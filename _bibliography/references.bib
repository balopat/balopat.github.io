@article{steudtner_quantum_2019,
	title = {Quantum codes for quantum simulation of {Fermions} on a square lattice of qubits},
	volume = {99},
	issn = {2469-9926, 2469-9934},
	url = {http://arxiv.org/abs/1810.02681},
	doi = {10.1103/PhysRevA.99.022308},
	abstract = {Quantum simulation of fermionic systems is a promising application of quantum computers, but in order to program them, we need to map fermionic states and operators to qubit states and quantum gates. While quantum processors may be built as two-dimensional qubit networks with couplings between nearest neighbors, standard Fermion-to-qubit mappings do not account for that kind of connectivity. In this work we concatenate the (one-dimensional) Jordan-Wigner transform with specific quantum codes defined under the addition of a certain number of auxiliary qubits. This yields a novel class of mappings with which any fermionic system can be embedded in a two-dimensional qubit setup, fostering scalable quantum simulation. Our technique is demonstrated on the two-dimensional Fermi-Hubbard model, that we transform into a local Hamiltonian. What is more, we adapt the Verstraete-Cirac transform and Bravyi-Kitaev Superfast simulation to the square lattice connectivity and compare them to our mappings. An advantage of our approach in this comparison is that it allows us to encode and decode a logical state with a simple unitary quantum circuit.},
	number = {2},
	urldate = {2022-03-15},
	journal = {Physical Review A},
	author = {Steudtner, Mark and Wehner, Stephanie},
	month = feb,
	year = {2019},
	note = {arXiv: 1810.02681},
	keywords = {Quantum Physics},
	pages = {022308},
}

@article{motta_low_2018,
	title = {Low rank representations for quantum simulation of electronic structure},
	url = {http://arxiv.org/abs/1808.02625},
	abstract = {The quantum simulation of quantum chemistry is a promising application of quantum computers. However, for N molecular orbitals, the \${\textbackslash}mathcal\{O\}(N{\textasciicircum}4)\$ gate complexity of performing Hamiltonian and unitary Coupled Cluster Trotter steps makes simulation based on such primitives challenging. We substantially reduce the gate complexity of such primitives through a two-step low-rank factorization of the Hamiltonian and cluster operator, accompanied by truncation of small terms. Using truncations that incur errors below chemical accuracy, we are able to perform Trotter steps of the arbitrary basis electronic structure Hamiltonian with \${\textbackslash}mathcal\{O\}(N{\textasciicircum}3)\$ gate complexity in small simulations, which reduces to \${\textbackslash}mathcal\{O\}(N{\textasciicircum}2 {\textbackslash}log N)\$ gate complexity in the asymptotic regime, while our unitary Coupled Cluster Trotter step has \${\textbackslash}mathcal\{O\}(N{\textasciicircum}3)\$ gate complexity as a function of increasing basis size for a given molecule. In the case of the Hamiltonian Trotter step, these circuits have \${\textbackslash}mathcal\{O\}(N{\textasciicircum}2)\$ depth on a linearly connected array, an improvement over the \${\textbackslash}mathcal\{O\}(N{\textasciicircum}3)\$ scaling assuming no truncation. As a practical example, we show that a chemically accurate Hamiltonian Trotter step for a 50 qubit molecular simulation can be carried out in the molecular orbital basis with as few as 4,000 layers of parallel nearest-neighbor two-qubit gates, consisting of fewer than 100,000 non-Clifford rotations. We also apply our algorithm to iron-sulfur clusters relevant for elucidating the mode of action of metalloenzymes.},
	urldate = {2022-03-13},
	journal = {arXiv:1808.02625 [physics, physics:quant-ph]},
	author = {Motta, Mario and Ye, Erika and McClean, Jarrod R. and Li, Zhendong and Minnich, Austin J. and Babbush, Ryan and Chan, Garnet Kin-Lic},
	month = aug,
	year = {2018},
	note = {arXiv: 1808.02625},
	keywords = {Physics - Computational Physics, Quantum Physics},
}

