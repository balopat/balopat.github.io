}

@article{reichardt_fault-tolerant_2018,
	title = {Fault-tolerant quantum error correction for {Steane}'s seven-qubit color code with few or no extra qubits},
	url = {https://arxiv.org/abs/1804.06995v1},
	doi = {10.1088/2058-9565/abc6f4},
	abstract = {Steane's seven-qubit quantum code is a natural choice for fault-tolerance experiments because it is small and just two extra qubits are enough to correct errors. However, the two-qubit error-correction technique, known as "flagged" syndrome extraction, works slowly, measuring only one syndrome at a time. This is a disadvantage in experiments with high qubit rest error rates. We extend the technique to extract multiple syndromes at once, without needing more qubits. Qubits for different syndromes can flag errors in each other. This gives equally fast and more qubit-efficient alternatives to Steane's error-correction method, and also conforms to planar geometry constraints. We further show that Steane's code and some others can be error-corrected with no extra qubits, provided there are at least two code blocks. The rough idea is that two seven-qubit codewords can be temporarily joined into a twelve-qubit code, freeing two qubits for flagged syndrome measurement.},
	language = {en},
	urldate = {2021-09-25},
	author = {Reichardt, Ben W.},
	month = apr,
	year = {2018},
}

@article{milne_quantum_2021,
