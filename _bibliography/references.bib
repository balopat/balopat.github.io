@article{bravyi_classical_2011,
	title = {Classical simulation of dissipative fermionic linear optics},
	url = {https://arxiv.org/abs/1112.2184v1},
	abstract = {Fermionic linear optics is a limited form of quantum computation which is known to be efficiently simulable on a classical computer. We revisit and extend this result by enlarging the set of available computational gates: in addition to unitaries and measurements, we allow dissipative evolution governed by a Markovian master equation with linear Lindblad operators. We show that this more general form of fermionic computation is also simulable efficiently by classical means. Given a system of \$N\$ fermionic modes, our algorithm simulates any such gate in time \$O(N{\textasciicircum}3)\$ while a single-mode measurement is simulated in time \$O(N{\textasciicircum}2)\$. The steady state of the Lindblad equation can be computed in time \$O(N{\textasciicircum}3)\$.},
	language = {en},
	urldate = {2022-01-26},
	author = {Bravyi, Sergey and Koenig, Robert},
	month = dec,
	year = {2011},
}

@article{bravyi_lagrangian_2005,
	title = {Lagrangian representation for fermionic linear optics},
	volume = {5},
	issn = {15337146, 15337146},
	url = {http://www.rintonpress.com/journals/doi/QIC5.3-3.html},
	doi = {10.26421/QIC5.3-3},
	abstract = {Notions of a Gaussian state and a Gaussian linear map are generalized to the case of anticommuting (Grassmann) variables. Conditions under which a Gaussian map is trace preserving and (or) completely positive are formulated. For any Gaussian map an explicit formula relating correlation matrices of input and output states is presented. This formalism allows to develop the Lagrangian representation for fermionic linear optics (FLO). It covers both unitary operations and the single-mode projectors associated with FLO measurements. Using the Lagrangian representation we reduce a classical simulation of FLO to a computation of Gaussian integrals over Grassmann variables. Explicit formulas describing evolution of a quantum state under FLO operations are put forward.},
