@article{bravyi_correcting_2018-1,
	title = {Correcting coherent errors with surface codes},
	volume = {4},
	copyright = {2018 The Author(s)},
	issn = {2056-6387},
	url = {https://www.nature.com/articles/s41534-018-0106-y},
	doi = {10.1038/s41534-018-0106-y},
	abstract = {Surface codes are building blocks of quantum computing platforms based on 2D arrays of qubits responsible for detecting and correcting errors. The error suppression achieved by the surface code is usually estimated by simulating toy noise models describing random Pauli errors. However, Pauli noise models fail to capture coherent processes such as systematic unitary errors caused by imperfect control pulses. Here we report the first large-scale simulation of quantum error correction protocols based on the surface code in the presence of coherent noise. We observe that the standard Pauli approximation provides an accurate estimate of the error threshold but underestimates the logical error rate in the sub-threshold regime. We find that for large code size the logical-level noise is well approximated by random Pauli errors even though the physical-level noise is coherent. Our work demonstrates that coherent effects do not significantly change the error correcting threshold of surface codes. This gives more confidence in the viability of the fault-tolerance architecture pursued by several experimental groups.},
	language = {en},
	number = {1},
	urldate = {2021-11-17},
	journal = {npj Quantum Information},
	author = {Bravyi, Sergey and Englbrecht, Matthias and König, Robert and Peard, Nolan},
	month = oct,
	year = {2018},
	keywords = {Computer science, Information theory and computation, Quantum information},
	pages = {1--6},
}

@article{daiha_quantum_2021,
	title = {A quantum number theory},
	url = {http://arxiv.org/abs/2108.10145},
	abstract = {We employ an algebraic procedure based on quantum mechanics to propose a `quantum number theory' (QNT) as a possible extension of the `classical number theory'. We built our QNT by defining pure quantum number operators (\$q\$-numbers) of a Hilbert space that generate classical numbers (\$c\$-numbers) belonging to discrete Euclidean spaces. To start with this formalism, we define a 2-component natural \$q\$-number \${\textbackslash}textbf\{N\}\$, such that \${\textbackslash}mathbf\{N\}{\textasciicircum}\{2\} {\textbackslash}equiv N\_\{1\}{\textasciicircum}\{2\} + N\_\{2\}{\textasciicircum}\{2\}\$, satisfying a Heisenberg-Dirac algebra, which allows to generate a set of natural \$c\$-numbers \$n {\textbackslash}in {\textbackslash}mathbb\{N\}\$. A probabilistic interpretation of QNT is then inferred from this representation. Furthermore, we define a 3-component integer \$q\$-number \${\textbackslash}textbf\{Z\}\$, such that \${\textbackslash}mathbf\{Z\}{\textasciicircum}\{2\} {\textbackslash}equiv Z\_\{1\}{\textasciicircum}\{2\} + Z\_\{2\}{\textasciicircum}\{2\} + Z\_\{3\}{\textasciicircum}\{2\}\$ and obeys a Lie algebra structure. The eigenvalues of each \${\textbackslash}textbf\{Z\}\$ component generate a set of classical integers \$m {\textbackslash}in {\textbackslash}mathbb\{Z\}{\textbackslash}cup {\textbackslash}frac\{1\}\{2\}{\textbackslash}mathbb\{Z\}{\textasciicircum}\{*\}\$, \${\textbackslash}mathbb\{Z\}{\textasciicircum}\{*\} = {\textbackslash}mathbb\{Z\} {\textbackslash}setminus {\textbackslash}\{0{\textbackslash}\}\$, albeit all components do not generate \${\textbackslash}mathbb\{Z\}{\textasciicircum}3\$ simultaneously. We interpret the eigenvectors of the \$q\$-numbers as `\$q\$-number state vectors' (QNSV), which form multidimensional orthonormal basis sets useful to describe state-vector superpositions defined here as qu\$n\$its. To interconnect QNSV of different dimensions, associated to the same \$c\$-number, we propose a quantum mapping operation to relate distinct Hilbert subspaces, and its structure can generate a subset \$W {\textbackslash}subseteq {\textbackslash}mathbb\{Q\}{\textasciicircum}\{*\}\$, the field of non-zero rationals. In the present description, QNT is related to quantum computing theory and allows dealing with nontrivial computations in high dimensions.},
	urldate = {2021-11-16},
	journal = {arXiv:2108.10145 [math-ph, physics:quant-ph]},
	author = {Daiha, Lucas and Rivelino, Roberto},
	month = aug,
	year = {2021},
	note = {arXiv: 2108.10145},
	keywords = {Mathematical Physics, Quantum Physics},
}

