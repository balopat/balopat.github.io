}

@article{huang_information-theoretic_2021,
	title = {Information-theoretic bounds on quantum advantage in machine learning},
	url = {http://arxiv.org/abs/2101.02464},
	abstract = {We study the performance of classical and quantum machine learning (ML) models in predicting outcomes of physical experiments. The experiments depend on an input parameter \$x\$ and involve execution of a (possibly unknown) quantum process \${\textbackslash}mathcal\{E\}\$. Our figure of merit is the number of runs of \${\textbackslash}mathcal\{E\}\$ required to achieve a desired prediction performance. We consider classical ML models that perform a measurement and record the classical outcome after each run of \${\textbackslash}mathcal\{E\}\$, and quantum ML models that can access \${\textbackslash}mathcal\{E\}\$ coherently to acquire quantum data; the classical or quantum data is then used to predict outcomes of future experiments. We prove that for any input distribution \${\textbackslash}mathcal\{D\}(x)\$, a classical ML model can provide accurate predictions on average by accessing \${\textbackslash}mathcal\{E\}\$ a number of times comparable to the optimal quantum ML model. In contrast, for achieving accurate prediction on all inputs, we prove that exponential quantum advantage is possible. For example, to predict expectations of all Pauli observables in an \$n\$-qubit system \${\textbackslash}rho\$, classical ML models require \$2{\textasciicircum}\{{\textbackslash}Omega(n)\}\$ copies of \${\textbackslash}rho\$, but we present a quantum ML model using only \${\textbackslash}mathcal\{O\}(n)\$ copies. Our results clarify where quantum advantage is possible and highlight the potential for classical ML models to address challenging quantum problems in physics and chemistry.},
	urldate = {2021-05-15},
	journal = {arXiv:2101.02464 [quant-ph]},
	author = {Huang, Hsin-Yuan and Kueng, Richard and Preskill, John},
	month = apr,
	year = {2021},
	note = {arXiv: 2101.02464},
	keywords = {Computer Science - Information Theory, Computer Science - Machine Learning, Quantum Physics},
}

@article{zhang_exact_2003,
	title = {Exact two-qubit universal quantum circuit},
	volume = {91},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/quant-ph/0212109},
	doi = {10.1103/PhysRevLett.91.027903},
	abstract = {We provide an analytic way to implement any arbitrary two-qubit unitary operation, given an entangling two-qubit gate together with local gates. This is shown to provide explicit construction of a universal quantum circuit that exactly simulates arbitrary two-qubit operations in SU(4). Each block in this circuit is given in a closed form solution. We also provide a uniform upper bound of the applications of the given entangling gates, and find that exactly half of all the Controlled-Unitary gates satisfy the same upper bound as the CNOT gate. These results allow for the efficient implementation of operations in SU(4) required for both quantum computation and quantum simulation.},
	number = {2},
	urldate = {2021-05-07},
	journal = {Physical Review Letters},
	author = {Zhang, Jun and Vala, Jiri and Sastry, Shankar and Whaley, K. Birgitta},
	month = jul,
	year = {2003},
	note = {arXiv: quant-ph/0212109},
	keywords = {Quantum Physics},
