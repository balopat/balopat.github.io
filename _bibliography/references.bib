@article{kastoryano_precisely_2013,
	title = {Precisely timing dissipative quantum information processing},
	volume = {110},
	issn = {0031-9007, 1079-7114},
	url = {http://arxiv.org/abs/1205.0985},
	doi = {10.1103/PhysRevLett.110.110501},
	abstract = {Dissipative engineering constitutes a framework within which quantum information processing protocols are powered by system-environment interaction rather than by unitary dynamics alone. This framework embraces noise as a resource, and consequently, offers a number of advantages compared to one based on unitary dynamics alone, e.g., that the protocols are typically independent of the initial state of the system. However, the time independent nature of this scheme makes it difficult to imagine precisely timed sequential operations, conditional measurements or error correction. In this work, we provide a path around these challenges, by introducing basic dissipative gadgets which allow us to precisely initiate, trigger and time dissipative operations, while keeping the system Liouvillian time-independent. These gadgets open up novel perspectives for thinking of timed dissipative quantum information processing. As an example, we sketch how measurement based computation can be simulated in the dissipative setting.},
	number = {11},
	urldate = {2022-11-08},
